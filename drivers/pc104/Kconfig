#
# PC104 framework configuration
#

menu "PC104 Support"

config PC104
	tristate "PC104 Bus support"
	help
	  Say Y here if you want to support PC104 peripherals

if PC104

config PC104_TSRELAY8
	tristate "TS-RELAY8 Driver"
	help
	  Enables support for the TS-RELAY8 with 8 relays

endif # PC104

endmenu
